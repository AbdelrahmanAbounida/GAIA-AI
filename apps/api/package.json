{
  "name": "@gaia/api",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.cjs"
    },
    "./types": {
      "import": {
        "types": "./dist/types.d.ts",
        "default": "./dist/types.js"
      },
      "require": "./dist/types.cjs"
    },
    "./ratelimit": {
      "import": {
        "types": "./dist/ratelimits.d.ts",
        "default": "./dist/ratelimits.js"
      },
      "require": "./dist/ratelimits.cjs"
    },
    "./utils": {
      "import": {
        "types": "./dist/utils.d.ts",
        "default": "./dist/utils.js"
      },
      "require": "./dist/utils.cjs"
    },
    "./scalar": {
      "import": {
        "types": "./dist/scalar.d.ts",
        "default": "./dist/scalar.js"
      },
      "require": "./dist/scalar.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist .turbo node_modules"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "typescript": "^5.6.0"
  },
  "keywords": [
    "db",
    "typescript"
  ],
  "dependencies": {
    "@gaia/ai": "workspace:^",
    "@gaia/db": "workspace:^",
    "@orpc/client": "^1.12.2",
    "@orpc/openapi": "^1.12.2",
    "@orpc/server": "^1.12.2",
    "@orpc/zod": "^1.12.2",
    "axios": "^1.13.2",
    "nanoid": "^5.1.6",
    "uuid": "^13.0.0",
    "vm2": "^3.10.0",
    "zod": "^4.1.12"
  }
}
